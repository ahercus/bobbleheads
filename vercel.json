{
    "version": 2,
    "builds": [
      {
        "src": "api/generate.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/generate.js"
      },
      {
        "src": "/styles.css",
        "dest": "/styles.css"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }